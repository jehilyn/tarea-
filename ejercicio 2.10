#include <stdio.h>
#include <math.h>

int main(void)
{
  /* par, impar o nulo.

  NUM: variable de tipo entero. */

  int NUM;
  printf("Ingrese el numero: ");
  scanf("%d", &NUM);
  if (NUM == 0)
    printf("\nNulo");
  else
    if (pow (-1, NUM)> 0)
        printf("\npar");
  else
    printf("\nImpar");
  }
